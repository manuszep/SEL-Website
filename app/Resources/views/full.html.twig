        <div class="header">
            <div class="wrapper">
                <div class="logo"><img src="/logo_white.svg" width="282" height="48" /></div>
                <ul class="menu">
                    <li><a href="/">Accueil</a></li>
                    <li><a href="#">Le SEL</a>
                        <ul>
                            <li><a href="/le-sel-athois">Le SEL Athois</a></li>
                            <li><a href="/articles">La vie du SEL</a></li>
                            <li><a href="/agenda">Agenda</a></li>
                            <li><a href="/contact">Contact</a></li>
                            <li><a href="{{ path('user_index') }}">Liste des membres</a></li>
                        </ul>
                    </li>
                    <li><a href="{{ path('service_index') }}">Services</a></li>
                    <li><a href="/faq">Questions fr√©quentes</a></li>
                    {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                        <li><a href="{{ path('user_show', { 'id': app.user.id }) }}"><svg class="icon icon-user"><use xlink:href="#icon-user"></use></svg> {{ app.user.username }}</a>
                            <ul>
                                <li><a href="{{ path('fos_user_security_logout') }}">{{ 'layout.logout'|trans({}, 'FOSUserBundle') }}</a></li>
                                <li><a href="{{ path('user_edit', { 'id': app.user.id }) }}">Modifier</a></li>
                            </ul>
                        </li>
                    {% else %}
                        <li><a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a></li>
                    {% endif %}
                </ul>
            </div>
        </div>

        <div class="main-content">
            {% set messages_list = app.session.flashBag.all %}
            {% if messages_list %}
                <div class="wrapper">
                    {% for type, messages in messages_list %}
                        {% for message in messages %}
                            <div class="{{ type }}">
                                {{ message|trans({}, 'FOSUserBundle')|raw }}
                            </div>
                        {% endfor %}
                    {% endfor %}
                </div>
            {% endif %}

            {% block fos_user_content %}{% endblock fos_user_content %}
            {% block body %}{% endblock %}
        </div>

        <div class="add-tool">
            <div class="wrapper">
                <a class="toggle" href="#"><svg class="icon icon-plus"><use xlink:href="#icon-plus"></use></svg><span class="text"> Ajouter</span></a>
                <ul>
                    <li><a href="{{ path('service_new') }}" title="Service"><svg class="icon icon-lightbulb"><use xlink:href="#icon-lightbulb"></use></svg>
                            <span class="text">Service</span></a></li>
                    <li><a href="/agenda/ajouter" title="&Eacute;venement"><svg class="icon icon-calendar"><use xlink:href="#icon-calendar"></use></svg>
                            <span class="text">&Eacute;venement</span></a></li>
                    <li><a href="/articles/ajouter" title="Article"><svg class="icon icon-file-text"><use xlink:href="#icon-file-text"></use></svg>
                            <span class="text">Article</span></a></li>
                    <li><a href="{{ path('exchange_new') }}" title="&Eacute;change"><svg class="icon icon-exchange"><use xlink:href="#icon-exchange"></use></svg>
                            <span class="text">&Eacute;change</span></a></li>
                    <li><a href="{{ path('user_new') }}" title="Membre"><svg class="icon icon-user"><use xlink:href="#icon-user"></use></svg>
                            <span class="text">Membre</span></a></li>
                </ul>
            </div>
        </div>
        <div class="dimmer"></div>

        {% block javascripts %}{% endblock %}
    </body>
</html>